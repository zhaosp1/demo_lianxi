<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
   namespace: 用于指定该映射文件需要映射的Dao接口
-->
<mapper namespace="com.example.springboot.repository.dao.CustomerDAO">
    <select id="findAll" resultType="com.example.springboot.component.pojo.Customer">
        select * from t_customer
    </select>
</mapper>